<%- include ('head_mess') %>
<table class="table table-striped table-primary">
        <thead>
          <tr>
            <th>Mess Roll No</th>
            <th>Date</th>
            <th>Meal type</th>
            <th>Response</th>
          </tr>
        </thead>
        <tbody>
          <%if (data.length >0){ %>
          <% data.forEach(function(row){ %>
          <tr>
            <td><%= row.mess_rollno %></td>
            <td><%= row.date %></td>
            <td><%= row.meal_type %></td>
            <td><%= row.response %></td>
          </tr>
          <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found</td>
                
              </tr>
            <% } %>
        </tbody>
      </table>
 <%- include ('footer') %>