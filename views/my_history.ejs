<%- include ('head_stu') %>
<table class="table table-striped table-primary">
        <thead>
          <tr>
            <th>Date</th>
            <th>Meal Type</th>
            <th>Response</th>
          </tr>
        </thead>
        <tbody>
          <%if (data[0].length >0){ %>
          <% data[0].forEach(function(row){ %>
          <tr>
            <td><%= row.date %></td>
            <td><%= row.meal_type %></td>
            <td><%= row.response %></td>
          </tr>
          <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found</td>
                
              </tr>
            <% } %>
        </tbody>
      </table>

<p>Net Amount to be paid is:<%=data[1]%></p>
 <%- include ('footer') %>