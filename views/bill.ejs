
<link rel = "stylesheet" href = "common_home.css">
<%- include ('head_mess') %>
<table class="table table-striped table-primary">
        <thead>
          <tr>
            <th>Mess Roll No</th>
            <th>Amount to be paid</th>
          </tr>
        </thead>
        <tbody>
          <%if (data[0].length >0){ %>
          <% data[0].forEach(function(row){ %>
          <tr>
            <td><%= row.mess_rollno %></td>
            <td><%= row.tot %></td>
          </tr>
          <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found!</td>
                
              </tr>
            <% } %>
        </tbody>
      </table>

<p> Net Amount earned:<%=data[1]%></p>
 <%- include ('footer') %>